.greenDivider {
    background: url("http://www.bgrazpisanie.com/public/css/images/middle/headerBackgr1.gif") repeat-x scroll 0 100% transparent;
}

.menuDivider {
    height: 5px;
    width: auto;
}

menuContainer {
    height: 27px;
    background: url("http://www.bgrazpisanie.com/public/css/images/middle/menu-bg.png") repeat-x scroll 0 0 transparent;

}

.menuContainer{
    height: 27px;
    background:url("http://www.bgrazpisanie.com/public/css/images/middle/menu-bg.png") 
}

.menu-tab {
    color:#e3e3e3;
    font-family:Tahoma,Verdana,Arial,sans-serif;
    font-size:12px;
    font-weight:700;
    height:34px;
    line-height:29px;
    padding-left:10px;
    padding-right:10px;
    text-decoration:none;
}

.menu-active-tab,.menu-tab:hover {
    color:#fff;
    text-shadow:1px 1px #000;
}


.sublineSearchBufferCellLink {
    color:inherit;
    text-decoration:none;
    line-height:21px;
}

.classic-buy-button {
    vertical-align: middle;
    display: block;
    text-align: center;
    text-decoration: none;
    font-weight: 700;
    color: #fff;
    font-size: 14px;
    padding: 12px 5px;
}

.classic-buy-button {
    -webkit-border-radius: 10px;
    -khtml-border-radius: 10px;
    -moz-border-radius: 10px;
    -ms-border-radius: 10px;
    -o-border-radius: 10px;
    border-radius: 14px;
}

box-corners-all {
    -webkit-border-radius: 10px;
    -khtml-border-radius: 10px;
    -moz-border-radius: 10px;
    -ms-border-radius: 10px;
    -o-border-radius: 10px;
    border-radius: 14px;
}
.layout-block-c {
    width:15%;
    float:right;
    margin: -10px 12px 0px 0px;
}

.red-button,.classic-buy-button {
    color:#fff;
    border:.1em solid #f53930;
    background:#e83d35;
    filter:progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#f04f47,endColorstr=#e83d35);
    -ms-filter:progid:DXImageTransform.Microsoft.gradient(startColorStr='#fbcfcc',EndColorStr='#e83d35');
    background-image:linear-gradient(#f04f47,#e83d35);
}

.red-button:hover,.classic-buy-button:hover {
    text-decoration:none!important;
    background:#f04f47;
    filter:progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#e83d35,endColorstr=#f04f47);
    -ms-filter:progid:DXImageTransform.Microsoft.gradient(startColorStr='#e83d35',EndColorStr='#f04f47');
    background-image:linear-gradient(#e83d35,#f04f47);
}

.dateTwoway {
