<html>
    <head>
        <link type="text/css" rel="stylesheet" media="all" href="./stylesheets/style.css">
        <link type="text/css" rel="stylesheet" href="./stylesheets/reserves.css">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta  http-equiv="Content-Type" content="text/html; charset=utf-8">
        <link rel="icon" type="image/png" href="./images/icon.png" />
        <script type="text/javascript" src="./js/datetimepicker_css.js"></script>
        <script type="text/javascript" src="./js/prototype.js"></script>
        <script type="text/javascript" src="./js/reservation.js"></script>
        <title>BTM version 1.1.3</title>
    </head>
    <body>
        <div class="wrapper">
            <div class="menu">
                {{menu}}
            </div>
            <div class="contents">
                <div class="scrollable">
                    <!-- начало на информационната част-->
                    {{page_contents}}
                    <!-- край на информационната част-->
                </div>
            </div>
        </div>
        <script>

            jQuery.noConflict();
            (function($) {
                $(function() {
                    $(".datepicker").datepicker({dateFormat: 'yy-mm-dd'}).val();
                });
            })(jQuery);

        </script>
    </body>
</html>